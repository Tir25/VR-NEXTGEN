{
  "name": "vr-nextgen-solutions",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:logos": "node scripts/optimize-logo.js",
    "optimize:css": "node scripts/optimize-css.js",
    "analyze:css": "node scripts/analyze-unused-css.js",
    "optimize:all": "npm run optimize:images && npm run optimize:logos && npm run optimize:css",
    "optimize:bundle": "node scripts/optimize-bundle.js",
    "build:optimized": "npm run optimize:all && npm run build && npm run optimize:bundle"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "framer-motion": "^12.23.22",
    "gsap": "^3.13.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
